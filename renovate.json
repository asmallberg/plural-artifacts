  "regexManagers": [
    {
      "fileMatch": [
        "(^|/|\\.)vendor_images$"
      ],
      "matchStrings": [
        "(?<depName>.*?):(?<currentValue>.*)"
      ],
      "versioningTemplate": "docker",
      "datasourceTemplate": "docker"
